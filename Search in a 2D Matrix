class Solution
{
    public boolean searchMatrix(int[][] matrix, int target) 
    {  int m=matrix.length;
     int i=0;
        int n=matrix[i].length;
        for(i=0;i<m;i++)
         {
            for(int j=0;j<n;j++)
              {
                if(matrix[i][j]==target)return true;
              }
         } return false;
    }
}
